<!-- Contact Us Email Form -->

<section id="contact">

<div class="container">

  <div class="row">
    <div class="col-lg-12 text-center">
      <h2 class="section-heading">Contact Us</h2>
      <h3 class="section-subheading text-muted">Lorem ipsum dolor sit amet consectetur.</h3>
    </div>
  </div>

  <div class="row">
    <div class="col-lg-12">
      <form name="sentMessage" id="contactForm" novalidate>
        <div class="row">

          <div class="col-md-6">
            <div class="form-group">
              <input type="text" class="form-control" placeholder="Your Name *" id="name" required data-validation-required-message="Please enter your name.">
              <p class="help-block text-danger"></p>
            </div>
            <div class="form-group">
              <input type="email" class="form-control" placeholder="Your Email *" id="email" required data-validation-required-message="Please enter your email address.">
              <p class="help-block text-danger"></p>
            </div>
            <div class="form-group">
              <input type="tel" class="form-control" placeholder="Your Phone *" id="phone" required data-validation-required-message="Please enter your phone number.">
              <p class="help-block text-danger"></p>
            </div>
          </div>

          <div class="col-md-6">
            <div class="form-group">
              <textarea class="form-control" placeholder="Your Message *" id="message" required data-validation-required-message="Please enter a message."></textarea>
              <p class="help-block text-danger"></p>
            </div>
          </div>

          <div class="col-md-12">
            <div class="form-group">
              <input type="text" class="form-control" placeholder="Street Address Line 1" id="street1">
            </div>
            <div class="form-group">
              <input type="text" class="form-control" placeholder="Street Address Line 2" id="street2">
            </div>
          </div>

          <div class="col-md-6">
            <div class="form-group">
              <input type="text" class="form-control" placeholder="City" id="city">
            </div>
          </div>

          <div class="col-md-2">
            <div class="form-group">
              <input type="text" class="form-control" placeholder="State" id="state">
            </div>
          </div>

          <div class="col-md-4">
            <div class="form-group">
              <input type="text" class="form-control" placeholder="Zip Code" id="zipcode">
            </div>
          </div>

          <div class="clearfix"></div>

          <div class="col-lg-12 text-center">
            <div id="success"></div>
            <button type="submit" class="btn btn-xl">Send Message</button>
          </div>

        </div><!-- end .row -->
      </form><!-- end form -->
    </div><!-- end .col -->
  </div><!-- end .row -->
</div><!-- end .container -->
</section><!-- end Contact Us Email Form -->
